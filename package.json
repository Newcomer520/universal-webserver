{
  "name": "wiprognosis-web-portal",
  "version": "0.4.1",
  "description": "Wiprognosis web portal v2, made of universal architecutre",
  "main": "index.js",
  "scripts": {
    "dev": "gulp dev",
    "clean": "rm -rf build",
    "copy": "better-npm-run copy",
    "bundle": "better-npm-run bundle",
    "client-build": "npm run clean && npm run copy && npm run bundle",
    "server-build": "better-npm-run server-build",
    "build": "npm run client-build && npm run server-build",
    "deploy": "pm2 start tools/server-preloader.js --no-daemon",
    "start-server": "better-npm-run start-server",
    "eslint": "better-npm-run eslint",
    "test": "ROOT=$(pwd) better-npm-run test",
    "win-test": "ava.sh",
    "test-watch": "mocha --compilers js:babel-core/register --require babel-polyfill -w --recursive",
    "test-cover": "babel-node ./node_modules/isparta/bin/isparta cover --report lcov --report text --report html ./node_modules/mocha/bin/_mocha -- ./test/demo-testing.js"
  },
  "betterScripts": {
    "copy": {
      "command": "mkdir build && cp -r src/app/public/ build/"
    },
    "bundle": {
      "command": "webpack -p --config tools/webpack.config.babel.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "test": {
      "command": "ava",
      "env": {
        "NODE_ENV": "test",
        "radiusip": "210.200.13.224",
        "radiusport": "10000"
      }
    },
    "server-build": {
      "command": "babel src -d build --copy-files",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start-server": {
      "command": "node build/server/server-preloader.js",
      "env": {
        "NODE_ENV": "production",
        "radiusip": "210.200.13.224",
        "radiusport": "10000"
      }
    }
  },
  "keywords": [
    "react",
    "redux",
    "redux-saga",
    "universal",
    "node",
    "isomorphic"
  ],
  "repository": {
    "type": "git",
    "url": "https://gitlab.mrd3.openlab.tw/ilun_chen/universal-webserver.git"
  },
  "author": "I-Lun Chen",
  "license": "ISC",
