{
  "name": "universal-webserver",
  "version": "0.0.1",
  "description": "A universal webserver",
  "main": "index.js",
  "scripts": {
    "dev": "gulp dev",
    "build": "better-npm-run build",
    "start-server": "better-npm-run start-server",
    "nodemon": "better-npm-run nodemon",
    "babel-node": "babel-node --stage 0",
    "dev3": "node-debug tools/server-preloader.js",
    "eslint": "better-npm-run eslint",
    "test": "mocha --compilers js:babel/register ./test/demo-testing.js",
    "test-cover": "babel-node ./node_modules/isparta/bin/isparta cover --report lcov --report text --report html ./node_modules/mocha/bin/_mocha -- ./test/demo-testing.js"
  },
  "betterScripts": {
    "development": {
      "command": "babel-node tools/run develop",
      "env": {
        "NODE_ENV": "development",
        "port": "8008",
        "elip": "210.200.13.224",
        "elport": "5500",
        "redisip": "210.200.13.224",
        "redisport": "5501"
      }
    },
    "build": {
      "command": "babel-node tools/run build",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start-server": {
      "command": "npm run build & node-debug tools/server-preloader",
      "env": {
        "NODE_ENV": "production",
        "elip": "210.200.13.224",
        "elport": "5500",
        "redisip": "210.200.13.224",
        "redisport": "5501"
      }
    },
    "nodemon": {
      "command": "nodemon ./src/server --exec babel-node"
    },
    "eslint": {
      "command": "node temp.js"
    }
  },
  "keywords": [
    "react",
    "redux",
    "universal",
    "node",
    "isomorphic"
  ],
  "repository": {
    "type": "git",
    "url": "https://gitlab.mrd3.openlab.tw/ilun_chen/universal-webserver.git"
  },
  "author": "I-Lun Chen",
  "license": "ISC",
