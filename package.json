{
  "name": "universal-webserver",
  "version": "0.0.1",
  "description": "A universal webserver",
  "main": "index.js",
  "scripts": {
    "dev": "better-npm-run development",
    "build": "better-npm-run build",
    "start-server": "better-npm-run start-server",
    "nodemon": "better-npm-run nodemon",
    "babel-node": "babel-node --stage 0",
    "dev3": "nodemon --exec babel-node -- src/server.js",
    "eslint": "better-npm-run eslint",
    "test": "./node_modules/mocha/bin/_mocha --compilers js:babel/register ./test/reducerTest.js"
  },
  "betterScripts": {
    "development": {
      "command": "babel-node tools/run develop",
      "env": {
        "NODE_ENV": "development",
        "port": "8008"
      }
    },
    "build": {
      "command": "babel-node tools/run build",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start-server": {
      "command": "npm run build & node tools/server-preloader",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "nodemon": {
      "command": "nodemon ./src/server --exec babel-node"
    },
    "eslint": {
      "command": "node temp.js"
    }
  },
  "keywords": [
    "react",
    "redux",
    "universal",
    "node",
    "isomorphic"
  ],
  "author": "I-Lun Chen",
  "license": "ISC",
  "devDependencies": {
    "assets-webpack-plugin": "^3.2.0",
    "autoprefixer": "^6.1.2",
    "babel": "^5.8.34",
    "babel-core": "^5.8.34",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^5.4.0",
    "babel-plugin-react-transform": "^1.1.1",
    "better-npm-run": "0.0.5",
    "bluebird": "^3.0.6",
    "browser-sync": "^2.10.0",
    "chai": "^3.4.1",
    "cookie-parser": "^1.4.0",
    "css-loader": "^0.23.0",
    "deep-freeze": "0.0.1",
    "del": "^2.2.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.0.0",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.5",
    "gaze": "^0.5.2",
    "gulp": "^3.9.0",
    "gulp-nodemon": "^2.0.4",
    "json-loader": "^0.5.4",
    "less": "^2.5.3",
    "less-loader": "^2.2.2",
    "lodash.merge": "^3.3.2",
    "mkdirp": "^0.5.1",
    "mocha": "^2.3.4",
    "ncp": "^2.0.0",
    "nodemon": "^1.8.1",
    "postcss-loader": "^0.8.0",
    "raw-loader": "^0.5.1",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.2.0",
    "replace": "^0.3.0",
    "source-map-support": "^0.4.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0",
    "webpack-isomorphic-tools": "^2.2.21"
  },
  "dependencies": {
    "history": "^1.13.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.1",
    "react-router": "^1.0.2",
    "redux": "^3.0.5",
    "redux-router": "^0.1.0",
    "redux-simple-router": "^1.0.0",
    "serialize-javascript": "^1.1.2"
  }
}
